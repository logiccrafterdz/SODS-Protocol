{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "version": {
            "type": "string",
            "pattern": "^\\d+\\.\\d+$"
        },
        "contracts": {
            "type": "object",
            "patternProperties": {
                "^0x[a-fA-F0-9]{40}$": {
                    "type": "object",
                    "properties": {
                        "deployer": {
                            "type": "string",
                            "pattern": "^0x[a-fA-F0-9]{40}$"
                        },
                        "block": {
                            "type": "integer",
                            "minimum": 0
                        },
                        "name": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "deployer",
                        "block"
                    ],
                    "additionalProperties": false
                }
            },
            "additionalProperties": false
        },
        "last_updated": {
            "type": "integer",
            "minimum": 0
        }
    },
    "required": [
        "version",
        "contracts"
    ],
    "additionalProperties": false
}